{% include 'incl/header.twig' %}

<body class=""{% if config.theme_config.widescreen %} class="widescreen" {% endif %}>
    {% include 'incl/nav.twig' %}

    <div class="main" role="main">
        <div class="technologypage" id="article" role="article">
            {{ content }}
        </div>
        <div class="aside">
            {% for page in pages("thechnologies")|sort_by("time")|reverse if not page.hidden %}
                <div class="post">
                    <h3><a href="{{ page.url }}">{{ page.title }}</a></h3>
                </div>
            {% endfor %}
        </div>
    </div>
    
    {% include 'incl/footer.twig' %}
</body>

</html>
